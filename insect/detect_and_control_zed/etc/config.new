# Endpoints for subscriptions interfaces
WhisperStreamTopic.Endpoints=tcp -p 17682


# Proxies for required interfaces
GridPlannerProxy = gridplanner:tcp -h localhost -p 18439
OmniRobotProxy = omnirobot:tcp -h localhost -p 12799
SpeechProxy = speech:tcp -h localhost -p 12150



# This property is used by the clients to connect to IceStorm.
TopicManager.Proxy=IceStorm/TopicManager:default -p 9999


Ice.Warn.Connections=0
Ice.Trace.Network=0
Ice.Trace.Protocol=0
Ice.MessageSizeMax=20004800



